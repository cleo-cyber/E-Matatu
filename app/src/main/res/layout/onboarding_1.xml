<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/page_onboarding_1_ek1"
	android:layout_marginTop="0dp"
	android:layout_marginStart="0dp"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">


	<View
		android:id="@+id/__bg__onboarding_1_ek2"
		android:layout_width="match_parent"
		android:layout_height="898dp"
		android:layout_marginStart="0dp"
		android:background="@drawable/majestic_lukas_ipsd4z55_rw_unsplash_1"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/e_mat"
		android:textSize="18sp"
		android:textColor="@color/e_mat_color"
		android:layout_marginLeft="20dp"
		android:layout_marginTop="50dp"
		android:layout_width="58dp"
		android:layout_height="35dp"
		android:text="@string/e_mat_string"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintStart_toStartOf="parent" />


	<TextView
		android:id="@+id/convenient_cheap_rides"
		android:layout_width="310dp"
		android:layout_height="110dp"
		android:layout_marginTop="50dp"
		android:text="@string/convenient_cheap_rides_string"
		android:textColor="@color/text_color"
