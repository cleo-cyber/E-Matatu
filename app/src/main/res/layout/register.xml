<androidx.constraintlayout.widget.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/page_register_ek1"
	android:layout_marginTop="0dp"
	android:layout_marginStart="0dp"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<View
		android:id="@+id/_bg__register_ek2"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:background="@drawable/_bg__register_ek2_shape"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


	<RelativeLayout
		android:id="@+id/close"
		android:layout_marginTop="50dp"
		android:layout_marginStart="20dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintStart_toStartOf="parent">

		<ImageView
			android:id="@+id/vector"
			android:src="@drawable/vector"
			android:contentDescription="vector"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp"
			android:layout_width="22dp"
			android:layout_height="22dp" />

		<ImageView
			android:id="@+id/vector_ek1"
			android:src="@drawable/vector_ek1"
			android:contentDescription="vector_ek1"
			android:layout_marginStart="5.17dp"
			android:layout_marginTop="5.17dp"
			android:layout_width="11.67dp"
			android:layout_height="11.67dp" />

	</RelativeLayout>


	<TextView
		android:id="@+id/register_ek3"
		android:layout_width="133dp"
		android:layout_height="58dp"
		android:layout_gravity="center_horizontal|center"
		android:layout_marginTop="250dp"
		android:gravity="center"
		android:text="@string/register_ek3_string"
		android:textColor="@color/text_color"
		android:textSize="36sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.194"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<RelativeLayout
		android:id="@+id/buttons_ek4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="671dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent">


		<TextView
			android:id="@+id/button_ek5"
			android:layout_width="332dp"
			android:layout_height="45dp"
			android:layout_alignParentStart="true"
			android:layout_gravity="center_horizontal|center"
			android:layout_marginStart="4dp"
			android:layout_marginTop="0dp"
			android:background="@drawable/button_container_ek5_shape"
			android:gravity="center"
			android:text="@string/button_ek5_string"
			android:textAlignment="center"
			android:textColor="@color/text_color"
			android:textSize="18sp" />

	</RelativeLayout>

	<TextView
		android:id="@+id/_already_have_an_account__login"
		android:layout_width="246dp"
		android:layout_height="31dp"
		android:layout_marginTop="741dp"
		android:text="@string/_already_have_an_account__login_string"
		android:textColor="@color/text_ek1_color"
		android:textSize="14sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/label_ek1"
		android:layout_width="100dp"
		android:layout_height="21dp"
		android:layout_marginTop="452dp"
		android:layout_marginEnd="256dp"
		android:text="@string/email_address"
		android:textColor="@color/label_color"
		android:textSize="12sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/placeholder_ek1"
		android:layout_width="314dp"
		android:layout_height="45dp"
		android:layout_marginTop="476dp"
		android:background="@drawable/input_container_ek1_shape"
		android:inputType="textEmailAddress"
		android:textColor="@color/text_ek1_color"
		android:textSize="20dp"
		android:textAlignment="center"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.556"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />


	<TextView
		android:layout_width="100dp"
		android:layout_height="22dp"
		android:layout_marginTop="548dp"
		android:layout_marginEnd="256dp"
		android:text="@string/password"
		android:textColor="@color/label_color"
		android:textSize="12sp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<EditText
		android:id="@+id/placeholder_ek3"
		android:layout_width="314dp"
		android:layout_height="45dp"
		android:layout_marginTop="52dp"
		android:background="@drawable/input_container_ek1_shape"
		android:inputType="textPassword"
		android:textColor="@color/text_ek1_color"
		android:textSize="20dp"
		android:textAlignment="center"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintHorizontal_bias="0.556"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/placeholder_ek1" />


</androidx.constraintlayout.widget.ConstraintLayout>





